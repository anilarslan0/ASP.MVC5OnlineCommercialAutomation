@using MvcOnlineTicariOtomasyon.Models.Class
@model List<InvoiceContent>
@{
    ViewBag.Title = "InvoiceContent";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Fatura Kalemleri</h2>
<br />
<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active"></li>
</ol>
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Açıklama</th>
        <th>Miktar</th>
        <th>Birim Fiyat</th>
        <th>Tutar</th>
    </tr>
    @foreach (var ınvoice in Model)
    {
        <tr>
            <td>@ınvoice.InvoiceContentId</td>
            <td>@ınvoice.Description</td>
            <td>@ınvoice.Piece</td>
            <td>@ınvoice.Price</td>
            <td>@ınvoice.Amount</td>
          
        </tr>
    }


</table>
<a href="/Invoice/YeniKalem/" class="btn btn-secondary">Yeni Kalem Girişi</a>

